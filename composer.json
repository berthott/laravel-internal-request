{
  "name": "berthott/laravel-internal-request",
  "description": "Laravel Helper for running internal requests",
  "license": "MIT",
  "authors": [
      {
          "name": "Jan Bladt",
          "email": "berthott@web.de"
      }
  ],
  "autoload" : {
      "psr-4" : {
          "berthott\\InternalRequest\\": "src"
      }
  },
  "autoload-dev": {
      "psr-4": {
          "berthott\\InternalRequest\\Tests\\": "tests"
      }
  },
  "extra" : {
      "laravel" : {
          "providers" : [
              "berthott\\InternalRequest\\InternalRequestServiceProvider"
          ]
      }
  },
  "scripts": {
        "test": "vendor/bin/phpunit"
  },
  "require-dev": {
    "orchestra/testbench": "^8.0",
    "phpunit/phpunit": "^10.0",
    "doctrine/dbal": "^3.3"
  }
}
